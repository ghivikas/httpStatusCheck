<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class
name="com.http.bean.Httpbean"
table="Credential">

<id name="UserName" type="java.lang.String" 
column="UserName" length="10">

</id>




<property name="UserName" type="java.lang.String" column="Username" length="30"  insert="false" update="false"/>

<property name="password" type="java.lang.String" column="Password" length="200" />

<property name="Domain" type="java.lang.String" column="Domain" length="200" />

</class>


</hibernate-mapping>

